{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": ["security"],
    "extends": ["airbnb-base","plugin:security/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus":0,
        "no-console": 0,
        "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
         ]
    }
}
