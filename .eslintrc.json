{
    "env": {
        "browser": true,
        "node": true,
        "jest":true
    },
    "extends": ["airbnb-base","eslint:recommended","plugin:node/recommended"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-plusplus":0,
        "no-console": 0,
        "no-unused-vars": ["error", { "vars": "all", "args": "none", "ignoreRestSiblings": false }],
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "always"
            }
         ]
    }
}
